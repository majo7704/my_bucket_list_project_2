{{>nav_bar_not_member}}
<div class="cards-container">
  <ul class="cards">
    {{#each photo }}
      <li class="card"><img class="card__thumb" src="//live.staticflickr.com/{{this.server}}/{{this.id}}_{{this.secret}}.jpg" />
        {{!-- <button class="btn add btn--cta card__footer">Add goal</button> --}}
        <a href="/myGoals?server={{this.server}}&idApi={{this.id}}&secret={{this.secret}}&title={{this.title}}"><button class="btn add btn--cta card__footer">Add goal</button></a>
      </li>
    {{/each}}
  </ul>
</div>