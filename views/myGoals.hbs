{{> nav_bar_not_member}}
<div class="ultimate-cards-container">
  <div class="cards-container">
    
    {{#if user.goals}}
    <h1>Unfinished goals</h1>
    <ul class=" bare-list cards">
        {{#each user.goals}}
          {{> goalPartial this}}  
        {{/each}}
    </ul>
    {{/if}}
  </div>
  <div class="cards-container">
    {{#if user.completedGoals}}
    <h1>Completed goals</h1>
    <ul class=" bare-list cards">
      {{#each user.completedGoals}}
      {{> goalPartial this}}
      {{/each}}
      </ul>
    {{/if}}
  </div>
</div>